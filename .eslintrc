{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "never"
    ],
    "jsx-quotes": [2, "double"]
  }
}
